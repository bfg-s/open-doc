{
  "name": "bfg/open-doc",
  "description": "A open documentation generator for Laravel",
  "license": "MIT",
  "keywords": [
    "open",
    "doc"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Xsaven",
      "email": "gms.sw.xsaven@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-dom": "*",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
    "bfg/attributes": "^1.2",
    "bfg/repository": "^1.3.1",
    "bfg/comcode": "^1.1"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0|^7.0|^8.0",
    "phpunit/phpunit": "^8.4|^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Bfg\\OpenDoc\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bfg\\OpenDoc\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bfg\\OpenDoc\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
